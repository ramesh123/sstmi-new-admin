+                    style={{
+                        marginLeft: '1rem',
+                        background: 'none',
+                        border: 'none',
+                        color: 'white',
+                        fontSize: '1.5rem',
+                        cursor: 'pointer',
+                        padding: '0',
+                        lineHeight: '1'
